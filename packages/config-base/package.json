{
  "name": "@tamagui/config-base",
  "version": "1.0.1-beta.204",
  "types": "./types/index.d.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "sideEffects": false,
  "files": [
    "src",
    "types",
    "dist"
  ],
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "lint": "eslint",
    "lint:fix": "yarn lint --fix",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "dependencies": {
    "@tamagui/colors": "^1.0.1-beta.204",
    "@tamagui/core": "^1.0.1-beta.204",
    "@tamagui/font-inter": "^1.0.1-beta.204",
    "@tamagui/font-silkscreen": "^1.0.1-beta.204",
    "@tamagui/shorthands": "^1.0.1-beta.204",
    "@tamagui/theme-base": "^1.0.1-beta.204"
  },
  "devDependencies": {
    "@tamagui/build": "^1.0.1-beta.204"
  },
  "publishConfig": {
    "access": "public"
  }
}
