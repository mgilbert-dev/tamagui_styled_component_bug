{
  "name": "react-native-web-lite",
  "version": "1.0.1-beta.140",
  "description": "React Native for Web",
  "private": true,
  "main": "dist/cjs",
  "module": "dist/esm",
  "sideEffects": false,
  "files": [
    "types",
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tamagui-build --skip-types",
    "watch": "tamagui-build --watch --skip-types",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./dist/exports/Text/TextAncestorContext": "./dist/esm/exports/Text/TextAncestorContext.js",
    "./dist/modules/*": "./dist/esm/modules/*/index.js",
    "./dist/exports/*": "./dist/esm/exports/*/index.js",
    "./dist/*": "./dist/cjs/*/index.js"
  },
  "dependencies": {
    "create-react-class": "^15.7.0",
    "inline-style-prefixer": "^6.0.1",
    "normalize-css-color": "^1.0.2",
    "postcss-value-parser": "^4.2.0",
    "styleq": "^0.1.2"
  },
  "devDependencies": {
    "@tamagui/build": "^1.0.1-beta.141",
    "react": "*",
    "react-dom": "*"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "author": "Nate Wienert",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/tamagui/tamagui.git"
  },
  "tags": [
    "react"
  ],
  "keywords": [
    "react",
    "react-component",
    "react-native",
    "web"
  ]
}
